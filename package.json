{
  "name": "yim-crypto-app",
  "version": "1.0.0",
  "description": "My mini electorn app",
  "private": true,
  "author": "Nikolas Tomastik",
  "license": "ISC",
  "main": "src/electron/index.js",
  "scripts": {
    "start": "webpack serve --config ./src/webpack/webpack-dev.config.babel.js --disable-host-check --host 0.0.0.0 --env NODE_ENV=development --env PORT=$PORT",
    "build": "rm -rf build && webpack --config ./src/webpack/webpack-prod.config.babel.js --env NODE_ENV=production",
    "electron": "npx electron .",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 npx electron .",
    "electron-dev:win": "set ELECTRON_START_URL=http://localhost:5000 && npx electron .",
    "forge:start": "electron-forge start",
    "forge:package": "electron-forge package",
    "forge:make": "electron-forge make"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "core-js": "^3.15.2",
    "css-loader": "^5.2.6",
    "electron": "^13.1.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "mini-css-extract-plugin": "^2.0.0",
    "node-sass": "^6.0.1",
    "react-scripts": "^4.0.3",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "coinmarketcap-cryptocurrency-icons": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-number-format": "^4.6.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "yim-crypto-app",
            "iconUrl": "file://icon.ico",
            "loadingGif": "majo_spin.gif"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
